@page "/login"


<PageTitle>Login</PageTitle>

<h1>Faça o login</h1>

<p role="status">Quantidade de login: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Click aqui para fazer Login</button>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
    }
}


